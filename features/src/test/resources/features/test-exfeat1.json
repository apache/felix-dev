{
    "id" : "org.apache.sling:test-extension-feature:1.0.0",
    "extensions" : {
        "my-text-ex": {
            "kind": "optional",
            "text": [
            	"ABC", "DEF"
        	] 
        },
        "my-art-ex": {
            "kind": "mandatory", 
            "artifacts": [
            	{
					"id": "g:a:1",
					"my-md": 12345
				}, {
					"id": "g:a:zip:foobar:2"
				}
			]
        },
        "my-json-ex": {
           "kind": "transient", 
            "json": {"foo": [1, 2, 3] }
        }
    }
}